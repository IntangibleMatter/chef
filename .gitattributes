# Normalize EOL for all files that Git considers text files.
* text=auto eol=lf
*.aup filter=lfs diff=lfs merge=lfs -text
assets/banks/Desktop/* filter=lfs diff=lfs merge=lfs -text
assets/fmodproj/* filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
*.ogg filter=lfs diff=lfs merge=lfs -text
*.m4a filter=lfs diff=lfs merge=lfs -text
.raw/* filter=lfs diff=lfs merge=lfs -text
addons/android/* filter=lfs diff=lfs merge=lfs -text
addons/FMOD/native/lib/* filter=lfs diff=lfs merge=lfs -text
.raw/sfx/ filter=lfs diff=lfs merge=lfs -text
*.aup3 filter=lfs diff=lfs merge=lfs -text
*.pdc filter=lfs diff=lfs merge=lfs -text
